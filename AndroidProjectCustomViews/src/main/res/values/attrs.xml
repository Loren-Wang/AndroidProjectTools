<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SudokuSwipeGesturesView">
        <attr name="circleEffectiveRadius" format="dimension"/>
        <attr name="circleCenterRadius" format="dimension"/>
        <attr name="connectingLineWidth" format="dimension"/>
        <attr name="circleBorderWidth" format="dimension"/>
        <attr name="circleSelectedColor" format="color"/>
        <attr name="circleUnSelectedColor" format="color"/>
        <attr name="circleSelectedErrorColor" format="color"/>
        <attr name="isShowTrack" format="boolean"/>
        <attr name="circleShowType">
            <enum name="CIRCLE_SHOW_TYPE_1" value="1"></enum>
            <enum name="CIRCLE_SHOW_TYPE_2" value="2"></enum>
        </attr>
    </declare-styleable>

</resources>
